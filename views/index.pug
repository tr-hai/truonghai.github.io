extends layout

block content
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    title #{title}
  body
   .container
    .row
     .normal-video
      video#video(width='80%', controls='')
        source(src='truonghai.github.io/public/videos/mov_bbb.mp4', type='video/mp4')
        source(src='truonghai.github.io/public/videos/mov_bbb.mp4', type='video/ogg')
      h1 Video with no filter
     .canvas-video
      canvas#canvas(style='border:1px solid #000000;', width='392', height='215.6')
      h1 Video with Gaussian filter
